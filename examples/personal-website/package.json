{
  "name": "readme.com-examples-personal-website",
  "version": "*",
  "description": "readme.com personal website example",
  "type": "module",
  "readme.com": {
    "syntaxHighlight": false
  },
  "scripts": {
    "build:site": "rm -rf out && pnpx readme.com --templatePath templates",
    "dev:site": "pnpx concurrently 'pnpx serve dist' 'pnpx nodemon --watch readme.md --exec pnpm build:site'"
  }
}
